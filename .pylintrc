[MASTER]
jobs=4
unsafe-load-any-extension=no
init-hook="from pylint.config import find_pylintrc; import os, sys; sys.path.append(os.path.dirname(find_pylintrc()))"

[REPORTS]
output-format=text

[MESSAGES CONTROL]
disable=
    C0114,
    C0115,
    C0116,
    R0902,
    R0914,   
    R0201,
    W0640,
    W0715,
    C0412,
    E1101,
    E1205,
    C0103,
    R1710,
    W0703

[BASIC]
min-public-methods=0
